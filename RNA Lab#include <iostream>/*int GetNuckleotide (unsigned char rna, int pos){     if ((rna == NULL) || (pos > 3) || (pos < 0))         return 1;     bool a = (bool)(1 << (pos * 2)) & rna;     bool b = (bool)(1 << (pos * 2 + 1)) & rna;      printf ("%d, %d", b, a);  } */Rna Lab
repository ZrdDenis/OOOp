int Get (unsigned char rna, int pos){
    if ((pos > 4) || (pos < 1))
        return 1;
    rna >>= 2 * (4 - pos);

    return rna & 0x03;

}

int main() {
    using namespace std;

    unsigned char rna = 0xFF;

    int pos;

    cin >> pos;
    cout << Get(rna, pos);

    return 0;
}
